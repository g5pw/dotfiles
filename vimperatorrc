set editor="/opt/local/bin/mvim -f"
set hintchars=hjklasdfgyuiopqwertnmzxcvb

set gui=none
set statuslinevisibility=visible

" Mappings
nmap gn <C-S-n>
nmap gN <C-S-p>

nmap <C-A-n> <C-S-n>
nmap <C-A-p> <C-S-p>

nmap ,/ :nohlsearch<CR>

nmap ,t :tabopen! 

nmap <c-l> :reload<CR>

colorscheme zenburn
" vim: set ft=vimperator:
